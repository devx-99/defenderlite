<Project Sdk="Microsoft.NET.Sdk.Worker">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>dotnet-DefenderLite-621dd96e-2304-4441-95a3-42d6fe2d5e62</UserSecretsId>

    <!-- Add runtime identifiers for x86, x64, ARM64 -->
